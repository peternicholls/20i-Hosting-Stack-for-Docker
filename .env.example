# 20i Stack Environment Configuration
# Copy this file to .env and adjust values as needed

# Code directory - REQUIRED
# This should point to your website/application code
CODE_DIR=./src

# Web server configuration
HOST_PORT=80
PHP_VERSION=8.4

# Database configuration
MYSQL_VERSION=10.6
MYSQL_PORT=3306
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=devdb
MYSQL_USER=devuser
MYSQL_PASSWORD=devpass

# phpMyAdmin
PMA_PORT=8081

# Optional: Additional environment variables
# COMPOSE_PROJECT_NAME=my-20i-project
